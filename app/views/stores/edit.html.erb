<div class="bg">

  <div class="register-wrapper">

    <div class="register-header">
      <div><%= link_to '<', root_path, class: "go-back" %></div>
    </div>
    <div class="add-item-block">
      <div class="page-title mb40">店舗情報の編集</div>
      <div class="error-block">
        <%= form_with model: @store, local: true do |f| %>
          <%= render 'shared/error_messages', model: f.object %> 
        <% end %> 
      </div>
        <%= render partial: 'store_form', locals: { store: @store } %>
      </div>
      <%= render 'shared/footer' %>
  </div>
</div>